{
  "name": "storage-map-mcp",
  "version": "0.1.1",
  "description": "MCP server for unified database and storage access",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "type": "module",
  "bin": {
    "storage-map-mcp": "build/index.js"
  },
  "files": [
    "build",
    "README.md",
    "LICENSE"
  ],
  "packageManager": "pnpm@10.18.0",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "tsx src/index.ts",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf build"
  },
  "keywords": [
    "mcp",
    "database",
    "mysql",
    "athena",
    "storage",
    "query"
  ],
  "author": "Hoon <ihello0720@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cyhoon/mcp-storage-map.git"
  },
  "bugs": {
    "url": "https://github.com/cyhoon/mcp-storage-map/issues"
  },
  "homepage": "https://github.com/cyhoon/mcp-storage-map#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@aws-sdk/client-athena": "^3.873.0",
    "@aws-sdk/client-s3": "^3.873.0",
    "@modelcontextprotocol/sdk": "^1.17.2",
    "dotenv": "^17.2.1",
    "joi": "^17.11.0",
    "lru-cache": "^10.1.0",
    "mysql2": "^3.14.3",
    "p-queue": "^8.0.1",
    "winston": "^3.11.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^24.2.1",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "@vitest/ui": "^1.1.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "vitest": "^1.1.0"
  },
  "engines": {
    "node": ">=24.13.1"
  }
}
